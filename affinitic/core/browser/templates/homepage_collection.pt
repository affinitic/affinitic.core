<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">
    <head>
    </head>
    <body>
        <div metal:fill-slot="content-title"> </div>
        <div metal:fill-slot="content-description"> </div>
        <div metal:fill-slot="content-core">
            <div id="homepage_view">
                <div id="shortcut" class="color-text">
                    <a href="#About" class="button"><label>A propos</label></a>
                    <a href="#Services" class="button"><label>Nos services</label></a>
                    <a href="#OpenSource" class="button"><label>Open source</label></a>
                    <a href="#Team" class="button"><label>Notre équipe</label></a>
                    <a href="#Number" class="button"><label>Number</label></a>
<!--                    <a href="#References" class="button"><label></label></a>-->
                    <a href="#Tools" class="button"><label>Nos outils</label></a>
                    <a href="#Contact" class="button"><label>Contact</label></a>
                </div>
                <div class="parallax">
                    <tal:block tal:repeat="brain brains" tal:define="brains python: view.getObjectTag('about')">
                    <tal:item define="item brain/getObject; img1 item/background1">
                    <div id="About" class="group_parallax color-text">
                        <div class="background" tal:attributes="style string: background-image:url(${item/absolute_url}/@@download/background1/${item/background1/filename})">
                        </div>
                        <div class="foreground">
                            <tal:image tal:define="image nocall:item/image"
                                 tal:condition="nocall:image">
                            <div class="image" tal:attributes="style string: background-image:url(${item/absolute_url}/@@download/image/${image/filename})"></div>
                            </tal:image>
                            <div class="text">
                                <h1 tal:content="item/title_portlet"></h1>
                                <div tal:content="structure item/description_portlet/output"></div>
<!--                                <a class="more" tal:attributes="href string:${item/absolute_url}">-->
<!--                                    Plus d'infos-->
<!--                                </a>-->
                            </div>
                        </div>
                    </div>
                    </tal:item>
                    </tal:block>

                    <div id="Services" class="group_parallax">
                        <div class="foreground">
<!--                            <h2>Nos services</h2>-->
                            <ul class="row" >
                            <tal:block tal:repeat="brain brains" tal:define="brains python: view.getObjectTag('service_item')">
                                <li>
                                <tal:item tal:define="item brain/getObject">
<!--                                <a tal:attributes="href string:${item/absolute_url}">-->
                                <tal:image tal:define="image nocall:item/image"
                                     tal:condition="nocall:image">
                                <div class="image" tal:attributes="style string: background-image:url(${item/absolute_url}/@@download/image/${image/filename})"></div>
                                </tal:image>
                                <h3 tal:content="item/title_portlet"></h3>
<!--                                <div class="justify">-->
<!--                                    <span tal:content="structure item/description_portlet/output"></span>-->
<!--                                </div>-->
<!--                                </a>-->
                                </tal:item>
                            </li>
                            </tal:block>
                            </ul>
                        </div>
                        <div class="background">
                        </div>
                    </div>

                    <tal:block tal:repeat="brain brains" tal:define="brains python: view.getObjectTag('opensource')">
                    <tal:item tal:define="item brain/getObject">
                    <div id="OpenSource" class="group_parallax">
                        <div class="foreground">
                            <tal:image tal:define="image nocall:item/image"
                             tal:condition="nocall:image">
                            <div class="image" tal:attributes="style string: background-image:url(${item/absolute_url}/@@download/image/${image/filename})"></div>
                            </tal:image>
                            <h2 tal:content="item/title"></h2>
                            <h4 tal:content="item/title_portlet"></h4>
                            <div tal:content="structure item/description_portlet/output"></div>
<!--                            <a class="more" tal:attributes="href string:${item/absolute_url}">-->
<!--                                Plus dinfos-->
<!--                            </a>-->
                        </div>
                        <div class="background">
                        </div>
                    </div>
                    </tal:item>
                    </tal:block>


                    <tal:team tal:define="team_items python: view.getObjectTag('team_folder')">
                    <tal:block tal:repeat="team_item team_items">
                    <div id="Team" class="group_parallax" tal:define="team_folder team_item/getObject">
                        <div class="background">
                        </div>
                        <div class="foreground">
                            <a tal:attributes="href string:${team_folder/absolute_url}">
<!--                            <h2 tal:content="team_folder/title"></h2>-->
                            <div class="slideshow" tal:define="brains python: view.getObjectTag('team_item')">
                                <ul id="lightSlider_team" tal:define="size python: 200*len(brains)" tal:attributes="style string: width: ${size}px">
                                    <tal:block tal:repeat="brain brains">
                                    <li>
                                        <tal:item tal:define="item brain/getObject">
                                        <tal:image tal:define="image nocall:item/photo"
                                    tal:condition="nocall:image">
                                        <div class="team-photo image mini" tal:attributes="style string: background-image:url(${item/absolute_url}/@@download/photo/${image/filename})"></div>
                                        <div class="team-name" tal:content="item/title"></div>
                                        <div class="team-description" tal:content="item/fonction"></div>
                                        </tal:image>
                                        </tal:item>
                                    </li>
                                    </tal:block>
                                </ul>
                            </div>
                            </a>
                        </div>
                    </div>
                    </tal:block>
                    </tal:team>

                    <div id="Number" class="group_parallax">
                        <tal:block tal:repeat="brain brains" tal:define="brains python: view.getObjectTag('number_folder')">
                        <tal:item tal:define="item brain/getObject; img1 item/background1; img2 item/background2">
                        <div class="foreground">
                            <h1 tal:content="item/title_portlet"></h1>
                            <div tal:content="structure item/description_portlet/output"></div>
                            <ul class="row">
                                <li tal:define="repo view/getNumberRepoGit" tal:condition="repo">
                                    <div class="fa fa-github fa-xl"></div>
                                    <h1 tal:content="repo"></h1>
                                </li>
                                <li tal:define="birth view/getBirthday" tal:condition="birth">
                                    <div class="fa fa-birthday-cake fa-xl"></div>
                                    <h1 tal:content="birth"></h1>
                                </li>
                                <li tal:define="team python: view.getObjectTag('team_item')" tal:condition="team">
                                    <div class="fa fa-male fa-xl"></div>
                                    <h1 tal:content="python: len(team)"></h1>
                                </li>
                                <tal:block tal:repeat="brain brains" tal:define="brains python: view.getObjectTag('number_item')">
                                <tal:item tal:define="item brain/getObject">
                                <li>
                                    <div tal:attributes="class item/title"></div>
                                    <h1 tal:content="item/description"></h1>
                                </li>
                                </tal:item>
                                </tal:block>
                            </ul>
                        </div>
                        <div class="middleground" tal:attributes="style string: background-image:url(${item/absolute_url}/@@download/background2/${item/background2/filename})"></div>
                        <div class="background" tal:attributes="style string: background-image:url(${item/absolute_url}/@@download/background1/${item/background1/filename})">
                        </div>
                        </tal:item>
                        </tal:block>
                    </div>




<!--                    <div id="References" class="group_parallax">-->
<!--                        <div class="background">-->
<!--                        </div>-->
<!--                        <div class="foreground">-->
<!--                            <h2>Références</h2>-->
<!--                        </div>-->
<!--                    </div>-->


                    <div id="Tools" class="group_parallax">
                        <div class="background">
                        </div>
                        <div class="foreground">
                            <div class="slideshow" tal:define="brains python: view.getObjectTag('tools_folder')">
                                <tal:block tal:repeat="brain brains">
                                <div tal:define="item brain/getObject">
                                <h2 tal:content="item/title"></h2>
                                <span tal:content="item/description"></span>
                                </div>
                                </tal:block>
                            </div>
                            <div class="slideshow" tal:define="brains python: view.getObjectTag('tools_item')">
                                <ul id="slider_tools" tal:define="size python: 200*len(brains)" tal:attributes="style string: width: ${size}px">
                                    <tal:block tal:repeat="brain brains">
                                    <li>
                                        <tal:item tal:define="item brain/getObject">
                                        <tal:image tal:define="image nocall:item/image"
                                    tal:condition="nocall:image">
                                        <div class="image small" tal:attributes="style string: background-image:url(${item/absolute_url}/@@download/image/${image/filename})"></div>
                                        <h3 class="title" tal:content="item/title"></h3>
                                        </tal:image>
                                        </tal:item>
                                    </li>
                                    </tal:block>
                                </ul>
                            </div>
                        </div>
                    </div>

                    <tal:block tal:repeat="brain brains" tal:define="brains python: view.getObjectType('contact')">
                    <tal:item define="item brain/getObject">
                    <div id="Contact" class="group_parallax">
                        <div class="foreground color-background">
                            <h2 tal:content="item/title_portlet"></h2>
                            <a class="more" tal:attributes="href string:${item/absolute_url}">
                                Plus dinfos
                            </a>
                        </div>
                        <div class="background">
                            <a href="https://www.google.com/maps/d/viewer?mid=1GMm11fQKGgKx4USqE_OIJ4Lu-0PjvAJu&hl=fr&usp=sharing" alt="Lien Google map">
                                <div id="map" tal:content="structure item/description_portlet/output"></div>
                            </a>
                        </div>
                    </div>
                    </tal:item>
                    </tal:block>
                </div>
            </div>
        </div>
    </body>
</html>
